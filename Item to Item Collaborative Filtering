snan(train.loc[:, userId]) == False].index\n","  filtered_train = train_scaled.loc[filtered_indices, :]\n","\n","  if len(filtered_train) > 0:\n","    if len(filtered_train) < k:\n","      k = len(filtered_train)\n","\n","    nn = nei.NearestNeighbors(n_neighbors=k, metric=\"cosine\", algorithm=\"brute\")\n","    nn.fit(filtered_train)\n","    close_indices = nn.kneighbors(test_user, return_distance=False)[0]\n","    std = filtered_train.iloc[close_indices, :][userId].mean()\n","    avg = train.loc[:, userId].mean()\n","    pred = avg + std\n","    preds.append(pred)"],"metadata":{"id":"8WbDSPLvPA3L"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["preds"],"metadata":{"id":"-M31Fkhwp5_w"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["testnew = pd.concat([test,pd.Series(preds)],axis=1)\n","testnew = testnew.rename(columns={0:\"rating\"})\n","testnew.head()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":206},"id":"7x6k7X-ySUnM","executionInfo":{"status":"ok","timestamp":1714860238797,"user_tz":-180,"elapsed":485,"user":{"displayName":"Aslı Yasemin Dönmez (Student)","userId":"06766573016441585277"}},"outputId":"dc8a6ce7-f1e1-4bd3-f819-3fb726c67c69"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["         IDs    rating\n","0   629_2683  3.136364\n","1    601_904  4.590909\n","2  5673_3717  2.909091\n","3  5616_1721  3.454545\n","4  1099_3697  2.681818"],"text/html":["\n","  <div id=\"df-10d4529f-558e-44c5-9650-da767897ca58\" class=\"colab-df-container\">\n","    <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n","    }\n","</style>\n","<table border=\"1\" class=\"dataframe\">\n","  <thead>\n","    <tr style=\"text-align: right;\">\n","      <th></th>\n",